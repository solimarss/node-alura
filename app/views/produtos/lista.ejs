<html>
    <head>
      <title>Título do Documento</title>
    </head>
    <body>
        <table >
            <tr>
              <th>Id</th>
              <th>Nome</th>
              <th>Descrição</th>
              <th>Preço</th>
            </tr>
            <%for(var i=0;i<lista.length;i++) {%>
              <tr>
                  <td><%=lista[i].id%></td>
                  <td><%=lista[i].titulo%></td>
                  <td><%=lista[i].descricao%></td>
                  <td><%=lista[i].preco%></td>
              </tr>
            <%}%>
          </table> 

    </body>
</html>